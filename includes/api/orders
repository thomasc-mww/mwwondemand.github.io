</span>
<span class="kn">import</span> <span class="nn">requests</span>


<span class="k">def</span> <span class="nf">send_request</span><span class="p">():</span>
    <span class="c1"># Create Order
</span>    <span class="c1"># POST https://api.mwwondemand.com/api/orders
</span>
    <span class="k">try</span><span class="p">:</span>
        <span class="n">response</span> <span class="o">=</span> <span class="n">requests</span><span class="p">.</span><span class="n">post</span><span class="p">(</span>
            <span class="n">url</span><span class="o">=</span><span class="s">"https://api.mwwondemand.com/api/orders"</span><span class="p">,</span>
